
<%- include('partials/_header') -%>
<body>
	
		<!-- Page content -->
		<main>
			<div class="container-fluid px-0">
      </div>
				<div class="mx-auto tm-content-container" style="background-image:url(<%= imageUrl %>);">		
					<div class="row  pb-3">
						<div class="col-12">
							<div class="mx-auto tm-about-text-container px-3">
								<h2 class="tm-page-title mb-4 tm-text-primary">Login</h2>
                                
							</div>							
						</div>						
					</div>
                    <div class="mx-auto pb-3 tm-about-text-container px-3">
                        <div class="row">
                            <div class="col-lg-12 mb-5">
                                <form id="contact-form" action="/login" method="POST" class="tm-contact-form" enctype="multipart/form-data">
                                  <div class="form-group">
                                    <input rows="8" name="email" class="form-control rounded-0" placeholder="Email" required/>
                                  </div>
                                  <div class="form-group">
                                    <input type="password" name="password" class="form-control rounded-0" placeholder="Password" required />
                                  </div>
                                  <div class="form-group mb-0">
                                    <button type="submit" class="btn btn-primary rounded-0 d-block ml-auto mr-0 tm-btn-animate tm-btn-submit tm-icon-submit"><span>Log In</span></button>
                                  </div>
                                </form>    
                            </div>
                        </div>  
                    </div>                                      			
				</div>
			</div>
		</main>
        
        <div class="container-fluid tm-content-container mx-auto pt-5">
			<!-- Subscribe form and footer links -->
            <%- include('partials/_footer') -%>	
		</div>		
	</div>

	<script src="/js/jquery-3.4.1.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/parallax.min.js"></script>    
</body>
</html>